<h1><%= @project.name %></h1>
<p>Material: <%= @project.material %></p>
<p>Challenge Theme: <%= @project.challenge.theme %></p>
<!-- @project.contestants.sum{|contestant| contestant} %> -->
<p>Number of Contestants: <%= @project.contestants.length%></p>
<p>Average Contestant Experience: <%= @project.average_experience %></p>

<%= form_tag("/projects/#{@project.id}", method: :post) do %>
  <p><%= label_tag :contestant_id %>
     <%= text_field_tag :contestant_id %></p>
<%= submit_tag "Add Contestant to Project"%>
<% end %>
